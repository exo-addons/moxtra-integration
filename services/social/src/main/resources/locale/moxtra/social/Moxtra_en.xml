<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <ECMContextMenu>
    <event>
      <EditInMoxtra>Open in Moxtra</EditInMoxtra>
    </event>
  </ECMContextMenu>
  <UIAddDocumentSelector>
    <msg>
      <moxtraSessionNotDefined>Moxtra session not defined</moxtraSessionNotDefined>
      <exoFileRequired>File required</exoFileRequired>
      <fileNotNode>File not a node</fileNotNode>
      <uploadError>Error uploading file. {0}</uploadError>
      <binderNotFound>Moxtra Binder cannot be found</binderNotFound>
      <binderSpaceNotFound>Cannot find a space associated with Moxtra Binder</binderSpaceNotFound>
      <alreadyPage>It is already a Moxtra page</alreadyPage>
    </msg>
  </UIAddDocumentSelector>
  <UIAddDocumentSelectorPopup>
    <title>
      <UIAddDocumentSelector>Select document</UIAddDocumentSelector>
    </title>
  </UIAddDocumentSelectorPopup>
  <!-- XXX workaround to avoid exception when updating meet event from Moxtra app juzu portlet -->
  <CalendarUIActivity>
    <msg>
      <fromDateTime_updated>Start date has been updated to: {0}.</fromDateTime_updated>
      <toDateTime_updated>End date has been updated to: {0}.</toDateTime_updated>
    </msg>
  </CalendarUIActivity>
</bundle>